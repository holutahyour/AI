#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/1 -Tahyour/1 - Repo/AI/node_modules/.pnpm/shadcn@3.5.0/node_modules/shadcn/dist/node_modules:/proc/cygdrive/c/1 -Tahyour/1 - Repo/AI/node_modules/.pnpm/shadcn@3.5.0/node_modules/shadcn/node_modules:/proc/cygdrive/c/1 -Tahyour/1 - Repo/AI/node_modules/.pnpm/shadcn@3.5.0/node_modules:/proc/cygdrive/c/1 -Tahyour/1 - Repo/AI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/1 -Tahyour/1 - Repo/AI/node_modules/.pnpm/shadcn@3.5.0/node_modules/shadcn/dist/node_modules:/proc/cygdrive/c/1 -Tahyour/1 - Repo/AI/node_modules/.pnpm/shadcn@3.5.0/node_modules/shadcn/node_modules:/proc/cygdrive/c/1 -Tahyour/1 - Repo/AI/node_modules/.pnpm/shadcn@3.5.0/node_modules:/proc/cygdrive/c/1 -Tahyour/1 - Repo/AI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/shadcn@3.5.0/node_modules/shadcn/dist/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/shadcn@3.5.0/node_modules/shadcn/dist/index.js" "$@"
fi
